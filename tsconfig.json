{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@constants/*": ["constants/*"],
      "@features/*": ["features/*"],
      "@pages/*": ["pages/*"],
      "@layouts/*": ["layouts/*"],
    },
    "types": ["bun-types", "nuxt", "vue-router"]
  },
  "exclude": [
    "node_modules"
  ]
}
